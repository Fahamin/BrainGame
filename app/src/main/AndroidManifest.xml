<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="fff.phot.seeker.braingame">

    <uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:allowBackup="true"
        android:icon="@drawable/unnamed512x512"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".allactivity.HomeScreenActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".allactivity.GameScreenActivity"
            android:parentActivityName=".allactivity.DifficultySelectionScreenActivity"/>

        <activity android:name=".allactivity.DifficultySelectionScreenActivity"
            android:parentActivityName=".allactivity.HomeScreenActivity"></activity>
    </application>

</manifest>